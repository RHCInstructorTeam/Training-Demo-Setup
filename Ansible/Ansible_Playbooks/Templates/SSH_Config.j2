Host {{ course_sku }}_*
  StrictHostKeyChecking no
  ForwardAgent yes
  ForwardX11 no
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/lab_rsa

Host {{ course_sku }}_classroom
  User instructor
  Hostname {{ ROLE_IP }}
  IdentityFile ~/.ssh/gls_instructor_id_rsa
  Port 22022

Host {{ course_sku }}_bastion
  Hostname 172.25.252.1
  User student
  ProxyJump {{ course_sku }}_classroom

